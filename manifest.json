{
  "manifest_version": 3,
  "name": "FK - Import",
  "version": "1.0",
  "permissions": ["activeTab"],
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
      }
    },
    "icons": {
      "16": "icons/icon.png",
      "48": "icons/icon.png",
      "128": "icons/icon.png"
    },
  "background": {
    "service_worker": "background.js"
  }
}